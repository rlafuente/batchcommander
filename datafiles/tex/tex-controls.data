        type: number
        value: 200
        min: 0
        max: 10000
        increment: 100
        decimals: 0
    - emergencystretch:
        longname: Allow spacing
        type: number
        value: 0
        unit: pt
        min: 0
        max: 30
        increment: 2
        decimals: 0
    - looseness:
        longname: Make para looser
        type: number
        value: 0
        min: -2
        max: 2
        increment: 1
        decimals: 0
    - linepenalty:
