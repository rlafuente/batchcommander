Hyphenation:
    - hyphenpenalty:
        longname: Hyphenate or not
        type: number
        value: 50
        min: -10000
        max: 10000
        increment: 100
        decimals: 0
    - exhyphenpenalty:
        longname: Explicit hyphens
        type: number
        value: 50
        min: -10000
        max: 10000
        increment: 100
        decimals: 0
    - doublehyphendemerits:
        longname: Hyphenate adj. lines
        type: number
        value: 10000
        min: -10000
        max: 10000
        increment: 100
        decimals: 0
    - finalhyphendemerits:
        longname: Last line of para
        type: number
        value: 5000
        min: -10000
        max: 10000
        increment: 100
        decimals: 0
    - lefthyphenmin:
        longname: Before hyphen
        type: number
        value: 2
        min: 0
        max: 5
        increment: 1
        decimals: 0
    - righthyphenmin:
        longname: After hyphen
        type: number
        value: 3
        min: 0
        max: 5
        increment: 1
        decimals: 0

Paragraph:
    - parindent:
        longname: Paragraph indentation
        type: number
        value: 12
        unit: pt
        min: -12
        max: 12
        increment: 1
        decimals: 1
    - hsize:
        longname: Text width
        type: number
        value: 15
        unit: cm
        min: 5
        max: 20
        increment: .5
        decimals: 1
    - baselineskip:
        longname: baselineskip
        type: number
        value: 12
        unit: pt
        min: 1
        max: 40
        increment: 1
        decimals: 1
    - lineskiplimit:
        longname: Lines too close
        type: number
        value: 1
        unit: pt
        min: 0
        max: 5
        increment: 1
        decimals: 1
    - lineskip:
        longname: Extra spacing
        type: number
        value: 2
        unit: pt
        min: 0
        max: 5
        increment: 1
        decimals: 1
    - offinterlineskip:
        longname: No interline glue
        type: toggle
        value: off
    - overfullrule:
        longname: Show overful paras
        type: number
        value: 5
        unit: pt
        min: 0
        max: 10
        increment: 1
        decimals: 1
    - hfuzz:
        longname: Leniency in overfull
        type: number
        value: .1
        unit: pt
        min: 0
        max: 5
        increment: .1
        decimals: 1
    - raggedright:
        longname: Ragged right margin
        type: toggle
        value: off

