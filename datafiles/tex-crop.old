General:
    - Size:
        longname: Printing paper dimension
        type: choice
        value: "a0, a1, a2, a3, a4, a5, a6, b0, b1, b2, b3, b4, b5, b6, letter, legal, executive"
        increment: a4
    - center:
        longname: Center on paper
        type: toggle
        value: None
        increment: off
    - driver:
        longname: Driver
        type: choice
        value: "dvips, pdftex, pdflatex, vtex, nodriver, none"
        increment: nodriver
    - mirror:
        longname: Reflect document
        type: toggle
        value: None
        increment: off
    - rotate:
        longname: Rotate 180
        type: toggle
        value: None
        increment: off
    - invert:
        longname: White on black
        type: toggle
        value: None
        increment: off
